<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Location</th>
      <th>Privacity</th>
      <th>User</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.location %></td>
        <td><%= event.privacity %></td>
        <td><%= event.user_id %></td>
        <td><%= event.organization_id %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= link_to 'Comment',api_v1_organization_event_comments_path %></td>
        <td><%= link_to 'Comment',api_v1_organization_event_invitations_path %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


